package KMeans;

public class Term 
{
	String term;
	int docFreq;
	int totalFreq;
	int k;
	Double distance;
	
	public Term(String term)
	{
		this.term = term;
		docFreq   = 0;
		totalFreq = 0;
	}
	
	public void setTerm(String term)
	{
		this.term = term;
	}
	public void setDocFreq(int docFreq)
	{
		this.docFreq = docFreq;
	}
	public void setTotalFreq(int totalFreq)
	{
		this.totalFreq = totalFreq;
	}
	public void setClusterID(int k)
	{
		this.k = k;
	}
	public void setDistanceFromCentroid(Double distance)
	{
		this.distance = distance;
	}
	
	
	
	public String getTerm()
	{
		return this.term;
	}
	public int getDocFreq()
	{
		return this.docFreq;
	}
	public int getCollectionFreq()
	{
		return this.totalFreq;
	}
	public int getClusterID()
	{
		return this.k;
	}
	public Double getDistanceFromCentroid()
	{
		return this.distance;
	}
	
}