package KMeans;

public class Term 
{
	String term;
	int docFreq;
	int collectionFreq;
	
	public Term(String term)
	{
		this.term      = term;
		docFreq        = 0;
		collectionFreq = 0;
	}
	
	public String getTerm()
	{
		return this.term;
	}
	
	public int getDocFreq()
	{
		return this.docFreq;
	}

}